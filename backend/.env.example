# Database
DATABASE_URL=postgresql://user:password@localhost:5432/felend

# Application
SECRET_KEY=your-secret-key-here
DEBUG=True

# JWT
JWT_SECRET_KEY=your-jwt-secret-key-here
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Google OAuth & Forms API
# Получите эти данные в Google Cloud Console:
# 1. Создайте проект
# 2. Включите Google Forms API и Google OAuth2 API  
# 3. Создайте OAuth 2.0 Client ID
# 4. Добавьте redirect URI: http://localhost:8000/api/v1/auth/google/callback
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret